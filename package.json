{
  "name": "jest_practise_store",
  "version": "1.0.1",
  "description": "A store to be used when practising writing tests in jest.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "watch": "jest --watch",
    "build": "babel src/index.js --out-file index.js --presets=@babel/env",
    "build-docs": "jsdoc src/index.js -d build/jsdocs -t ./node_modules/minami",
    "build-docs-dev": "jsdoc src/index.js -d build/jsdocs -t ./node_modules/minami -p"
  },
  "keywords": [],
  "author": "Jake Hallam",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-jest": "^24.7.1",
    "jest": "^24.7.1",
    "jsdoc": "^3.5.5",
    "minami": "^1.2.3"
  }
}
